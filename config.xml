<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
    xmlns:gap   = "http://phonegap.com/ns/1.0"
    id          = "com.yoursite.basicparticle"
    version     = "1.03.06" >      

  <name>Photon-WebApp-Onide</name>
  <description>
      Web app creata da onide
  </description>
  
  <author email="daniele.tagliasco.its2019@gmail.com">
      Daniele Tagliasco o Onide
  </author>
  
  
  <gap:plugin name="cordova-plugin-whitelist" source="npm"/> 
  <plugin name="cordova-plugin-whitelist" spec="1.3.2" />
  
  
    
  <preference name='phonegap-version' value='cli-8.1.1' />
  <preference name='pgb-builder-version' value='2' />   
    

 
  <preference name='phonegap-version' value='cli-7.0.1' />
  <preference name="phonegap-version" value="cli-6.5.0" />  
  <!--<preference name="phonegap-version" value="3.7.0" />
  <preference name="phonegap-version" value="cli-6.3.0" />  
  <preference name="phonegap-version" value="cli-5.4.1" />  -->
 
 
  <preference name="orientation" value="default" />
  <preference name="fullscreen" value="true" />



  

  <preference name="android-installLocation" value="auto" />
  
  
  <!-- Android SDK versions 4.0=14  4.4=19,    5.1=22,  6.0=23,   7.0=24  -->
  
  
</widget>
